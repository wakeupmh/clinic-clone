    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.6.1",
    "express": "^4.17.1",
    "express-decorator-router": "^0.1.1",
    "helmet": "^3.22.0",
    "http-errors": "^1.8.0",
    "ioredis": "^4.22.0",
    "module-alias": "^2.2.2",
    "pg": "^8.3.3",
    "retry-axios": "^2.4.0",
    "sequelize": "^6.3.5",
